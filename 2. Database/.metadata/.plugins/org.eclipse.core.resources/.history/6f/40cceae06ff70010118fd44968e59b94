SELECT emp_no AS '사번', salary / 12 AS '월급' FROM salaries;
SELECT emp_no + 50, first_name FROM employees
WHERE first_name = '길동';

SELECT DISTINCT gender FROM employees;

-- salaries 테이블에서 salary 컬럼의 값을 월급이라고 가정
-- 1. 월급 (기존값)
-- 2. 예상 연봉(salary *12) 별칭은 "예상 연봉"
-- 3. 5% 인상 후의 예상 연봉(salary *12 * 1.05) 별칭은 "인상 후 예상 연봉"

SELECT salary , salary * 12 AS '예상 연봉', salary *12 *1.05 AS '인상 후 예상 연봉'FROM salaries;

SELECT emp_no, salary, from_date, to_date FROM salaries WHERE salary >= 80000;

SELECT * FROM employees
WHERE emp_no < 10005;

-- 입사일이 1960년 1월 1일 이후이면서 성별이 남자인 직원 조회